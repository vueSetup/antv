{
  "name": "@antv/x6-vue-components",
  "version": "0.1.0",
  "description": "Vue3 components for building x6 editors",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/**/style/*",
    "*.less"
  ],
  "scripts": {
    "clean": "rimraf dist es lib",
    "lint": "run-s lint:ts lint:style",
    "lint:ts": "eslint 'src/**/*.{js,ts}?(x)' --fix",
    "lint:style": "stylelint 'src/**/*.less' --syntax less --fix",
    "build:umd": "vue-tsc --noEmit && vite build",
    "build:types": "vue-tsc -p tsconfig.declaration.json",
    "build:less": "node ./scripts/less.js",
    "build": "run-s build:umd build:types build:less",
    "prebuild": "run-s lint clean",
    "precommit": "lint-staged"
  },
  "keywords": [
    "component",
    "components",
    "vue-components",
    "x6",
    "x6-editor",
    "editor",
    "ui",
    "antv"
  ],
  "inherits": [
    "@antv/x6-package-json/cli.json",
    "@antv/x6-package-json/less.json",
    "@antv/x6-package-json/eslint.json"
  ],
  "peerDependencies": {
    "ant-design-vue": ">=3.2.0",
    "vue": ">=3.2.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@vitejs/plugin-legacy": "^2.1.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "ant-design-vue": "^3.2.13",
    "fs-extra": "^10.0.0",
    "less": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2",
    "vite": "^3.1.8",
    "vue": "^3.2.41",
    "vue-tsc": "^0.40.13",
    "vue-types": "^3.0.2"
  },
  "author": {
    "name": "beachburg",
    "email": "beachburg@qq.com"
  },
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/antvis/x6",
  "bugs": {
    "url": "https://github.com/antvis/x6/issues"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/antvis/x6.git",
    "directory": "packages/x6-vue-components"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
