{
  "name": "@antv/x6-vue-components",
  "version": "1.0.0",
  "description": "Vue3 components for building x6 editors",
  "main": "src/index.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "scripts": {
    "clean": "rimraf dist es lib",
    "build:less": "node ./scripts/less.js",
    "build:types": "vue-tsc -p tsconfig.declaration.json",
    "build:ts": "vue-tsc --noEmit && vite build",
    "build":"run-s build:ts build:types build:less"
  },
  "keywords": [
    "component",
    "components",
    "vue-components",
    "x6",
    "x6-editor",
    "editor",
    "ui",
    "antv"
  ],
  "peerDependencies": {
    "ant-design-vue": ">=3.2.0",
    "vue": ">=3.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "ant-design-vue": "^3.2.10",
    "fs-extra": "^10.0.0",
    "less": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "vite": "^3.0.0",
    "vue": "^3.2.27",
    "vue-tsc": "^0.38.5",
    "vue-types": "^3.0.2"
  },
  "author": {
    "name": "shadow",
    "email": "buzzlightear@qq.com"
  },
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/antvis/x6",
  "bugs": {
    "url": "https://github.com/antvis/x6/issues"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/antvis/x6.git",
    "directory": "packages/x6-vue-components"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}